{
  "format_version": 2,
  "executors": [
    {
      "tasks": ["train"],
      "executor": {
        "path": "pneumonia_trainer.PneumoniaTrainer",
        "args": {
          "lr": 0.01,
          "epochs": 1,
          "train_task_name": "train"
        }
      }
    }
  ],
  "components": [
    {
      "id": "launcher",
      "path": "nvflare.app_common.launchers.subprocess_launcher.SubprocessLauncher",
      "args": {
        "script": "python3 custom/pneumonia_trainer.py",
        "launch_once": true
      }
    },
    {
      "id": "pipe",
      "path": "nvflare.fuel.utils.pipe.file_pipe.FilePipe",
      "args": {
        "mode": "PASSIVE",
        "root_path": "{WORKSPACE}/{JOB_ID}/{SITE_NAME}"
      }
    }
  ],
  "task_result_filters": [],
  "task_data_filters": []
}